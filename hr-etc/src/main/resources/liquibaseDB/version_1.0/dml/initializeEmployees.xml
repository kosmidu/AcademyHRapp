<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd">

    <changeSet id="init-data-product-types" author="kosmima" failOnError="true" runAlways="false" runOnChange="true">

        <comment>
            Initialize 'employee' table
        </comment>

        <insert tableName="employee">
            <column name="emp_id" valueSequenceNext="EMPLOYEE_SEQ"/>
            <column name="first_name" value="Laura" />
            <column name="last_name" value="Mendes" />
            <column name="phone_number" value="6999999999" />
            <column name="email" value="ma@fdg.com" />
            <column name="salary" value="900.50" />
            <column name="city" value="Athens" />
            <column name="country" value="Greece" />
            <column name="street" value="Panormou" />
            <column name="str_num" value="86" />
            <column name="zip_code" value="178765" />
            <column name="join_date" value="1999-07-25" />
            <column name="birth_date" value="1988-03-13" />
            <column name="dep_id" value="30" />
        </insert>
    </changeSet>

</databaseChangeLog>
